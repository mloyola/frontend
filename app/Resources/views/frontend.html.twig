{% extends '::base.html.twig' %}

{% block stylesheets %}     
    <link href="{{ asset(global.directorio_css ~ "bootstrap.min.css")}}" rel="stylesheet">
    <link href="{{ asset(global.directorio_css ~ "agency.css")}}" rel="stylesheet">
    <link href="{{ asset(global.directorio_font_awesome ~ "css/font-awesome.min.css")}}" rel="stylesheet" type="text/css">    
    <link href='http://fonts.googleapis.com/css?family=GeosansLight:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic' rel='stylesheet' type='text/css'>  
    <!-- Custom Fonts -->    
    <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href='http://fonts.googleapis.com/css?family=Kaushan+Script' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css'>  
{% endblock %}

{% block javascripts %}      
    <script src="{{ asset(global.directorio_js ~ "jquery-1.11.0.js")}}"></script>     
    <script src="{{ asset(global.directorio_js ~ "bootstrap.min.js")}}"></script>    

    <!-- Plugin JavaScript -->
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>

    <script src="{{ asset(global.directorio_js ~ "classie.js")}}"></script>    
    <script src="{{ asset(global.directorio_js ~ "cbpAnimatedHeader.js")}}"></script>    

    <!-- Contact Form JavaScript -->
    <script src="{{ asset(global.directorio_js ~ "jqBootstrapValidation.js")}}"></script>    
    <script src="{{ asset(global.directorio_js ~ "contact_me.js")}}"></script>  

    <!-- Custom Theme JavaScript -->
    <script src="{{ asset(global.directorio_js ~ "agency.js")}}"></script> 
{% endblock %}

{% block body_id %} id="page-top" class="index" {% endblock %}

{% block menu %}
    <!-- Navigation -->
    {% block header_barra %} <nav class="navbar navbar-default navbar-fixed-top"> {% endblock %}
    <!-- <nav class="navbar navbar-default navbar-fixed-top"> -->
      <div class="container">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header page-scroll">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
               
          <a class="navbar-brand page-scroll" href="#page-top"><img src="{{ asset(global.directorio_img ~ "logos/logo-ecoin-xssm.png")}}"></a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">          
          <ul class="nav navbar-nav navbar-right">
            <li class="hidden"><a href="#page-top"></a></li>

            {% block menu_item %}{% endblock %}

          </ul>          
        </div>
        <!-- /.navbar-collapse -->
      </div>
        <!-- /.container-fluid -->
    </nav>
{% endblock %}        

